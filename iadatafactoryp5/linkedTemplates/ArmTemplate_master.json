{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "iadatafactoryp5"
		},
		"AzureDataLakeStorageAdlsNycPayroll_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorageAdlsNycPayroll'"
		},
		"AzureDataLakeStorageSynapse_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorageSynapse'"
		},
		"AzureSqlDatabase1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase1'"
		},
		"AzureSynapseAnalytics1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSynapseAnalytics1'"
		},
		"AzureDataLakeStorageAdlsNycPayroll_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsnycpayrollisyea.dfs.core.windows.net/"
		},
		"AzureDataLakeStorageSynapse_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://iadatalakeacct.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "iadatafactoryp5_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorageAdlsNycPayroll_accountKey": {
						"value": "[parameters('AzureDataLakeStorageAdlsNycPayroll_accountKey')]"
					},
					"AzureDataLakeStorageSynapse_accountKey": {
						"value": "[parameters('AzureDataLakeStorageSynapse_accountKey')]"
					},
					"AzureSqlDatabase1_connectionString": {
						"value": "[parameters('AzureSqlDatabase1_connectionString')]"
					},
					"AzureSynapseAnalytics1_connectionString": {
						"value": "[parameters('AzureSynapseAnalytics1_connectionString')]"
					},
					"AzureDataLakeStorageAdlsNycPayroll_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorageAdlsNycPayroll_properties_typeProperties_url')]"
					},
					"AzureDataLakeStorageSynapse_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorageSynapse_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "iadatafactoryp5_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'iadatafactoryp5_ArmTemplate_0')]"
			]
		}
	]
}